{
    "@ryantam626/jupyterlab_code_formatter:settings": {
        "preferences": {
            "default_formatter": {
                "python": "yapf",
                "r": "formatR"
            }
        },
        "yapf": {
            "style_config": "facebook"
        }
    },
    "@jupyterlab/shortcuts-extension:shortcuts": {
        "shortcuts": [
            {
                "command": "jupyterlab_code_formatter:yapf",
                "keys": [
                    "Ctrl Shift Enter"
                ],
                "selector": ".jp-Notebook.jp-mod-editMode"
            }
        ]
    }
}